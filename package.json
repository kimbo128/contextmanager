{
  "name": "contextmanager",
  "version": "1.0.0",
  "description": "MCP Server implementations for various domains",
  "type": "module",
  "bin": {
    "mcp-server-contextmanager": "./dist/main/index.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "workspaces": [
    "main"
  ],
  "scripts": {
