  "bugs": {
    "url": "https://github.com/tejpalvirk/contextmanager/issues"
  },
  "homepage": "https://github.com/tejpalvirk/contextmanager#readme",
  "devDependencies": {
    "@types/node": "^18.15.3",
    "@types/yargs": "^17.0.33",
    "shx": "^0.3.4",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "chalk": "^5.4.1",
    "yargs": "^17.7.2"
  }
}
